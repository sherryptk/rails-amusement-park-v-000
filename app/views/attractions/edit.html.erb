<%= form_for @attraction, url: {:controller => :attractions, action: :update} do |f| %>
  Name: <%= f.text_field :name %><br />
  Height: <%= f.text_field :min_height %><br />
  Happiness: <%= f.text_field :happiness_rating %><br />
  Nausea: <%= f.text_field :nausea_rating %><br />
  Tickets: <%= f.text_field :tickets %><br />
  <%= f.submit "Update Attraction" %>
<% end %>
